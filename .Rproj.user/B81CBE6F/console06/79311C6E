## cmodel.frac and ddmodel should give same estimation results
source('~/cancer_pheno/cancer_pheno/ddmodel.R')
source('~/cancer_pheno/cancer_pheno/countLRT.R')
source('~/cancer_pheno/cancer_pheno/simulate.R')

load(paste0("/home/simingz/cancer_pheno/data_run/simulation_2019-04-29/sgdata.Rd"
))
load("~/cancer_somatic/data_run/combined_20170526_5/UCS/UCS_parameters_BMvar.Rdat
a")
bmrpars <- BMpars$fullpars[1:9] - log(50)
simdata <- simulate_1funcv(sgdata, bmrpars, betaf0=0.5, Nsample=1000, Nc=200, bet
a_gc=c(0,1), fracc=0.8, fracn=0.2)

BMR <- exp(simdata$bmrpars)
cmodel.frac(simdata$mutbyt, simdata$pheno, simdata$annodata, c(simdata$efsize$avb
etaf0, simdata$efsize$avbetaf1)) # diffDriver
cmodel.frac(simdata$mutbyt, simdata$pheno, simdata$annodata, c(simdata$efsize$avb
etaf0f1, 0)) # diffDriver-baseline
[7m--More--(60%)[27m[K
[7m--More--(60%)[27mmutmtx <- do.call(rbind, simdata$mutbyt)
[7m--More--(63%)[27me <- simdata$pheno
[7m--More--(64%)[27mbmrmtx <- do.call(rbind, lapply(1:length(simdata$mutbyt), function(t) matrix(simd
[7m--More--(70%)[27mata$bmrpars[t], nrow = dim(simdata$mutbyt[[t]])[1], ncol = dim(simdata$mutbyt[[t]
[7m--More--(76%)[27m])[2])))[K
[7m--More--(77%)[27mft <- do.call(rbind, simdata$annodata)$functypecode
[7m--More--(81%)[27mfe1 <- c("7" = simdata$efsize$avbetaf0, "8" = simdata$efsize$avbetaf0 + simdata$e
[7m--More--(87%)[27mfsize$avbetaf1)[ft]
[7m--More--(88%)[27mfe2 <- rep(simdata$efsize$avbetaf0f1, dim(mutmtx)[1])
[7m--More--(92%)[27m[K
[7m--More--(92%)[27mddmodel(mutmtx, e, bmrmtx, fe1)  # diffDriver
